<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="emma.basic.chessgameus"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="4" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:label="@string/app_name"
            android:name="emma.basic.chessgameus.ChessGameUSActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        
        <activity android:name="emma.basic.chessgameus.ChessBoard.ChessPlayActivity" >
        </activity>
        
        <activity android:name="emma.basic.chessgameus.OnlineMultiplayerMenu.MultiOnlineMenu" >
        </activity>
        
        <activity
            android:name="emma.basic.chessgameus.OnlineMultiplayerMenu.JoinGame"
            android:noHistory="true" >
        </activity>
        
        <activity
            android:name="emma.basic.chessgameus.OnlineMultiplayerMenu.CreateGame"
            android:noHistory="true" >
        </activity>
    </application>

</manifest>